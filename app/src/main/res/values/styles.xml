<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="ToggleButtonWithIcon" parent="Widget.MaterialComponents.Button.OutlinedButton">
        <item name="iconPadding">0dp</item>
        <item name="android:insetTop">0dp</item>
        <item name="android:insetLeft">0dp</item>
        <item name="android:insetRight">0dp</item>
        <item name="android:insetBottom">0dp</item>
        <item name="iconSize">25dp</item>
        <item name="iconGravity">textStart</item>
        <item name="android:minWidth">56dp</item>
        <item name="android:minHeight">56dp</item>
        <item name="strokeWidth">0dp</item>
        <item name="backgroundTint">@android:color/transparent</item>
        <item name="rippleColor">@android:color/transparent</item>
    </style>

    <style name="ShapeAppearanceOverlayExtended" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">16dp</item>
    </style>

    <style name="SpeedDialog" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:background">@color/indigo</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColorAlertDialogListItem">@color/white</item>
        <item name="android:windowMinWidthMajor">40%</item>
        <item name="android:dialogPreferredPadding">16dp</item>
    </style>

</resources>